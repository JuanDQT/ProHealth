<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="12dp"
    android:paddingRight="12dp"
    tools:context=".AjustesActivity"
    android:paddingBottom="12dp"
    android:paddingTop="12dp"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/tv_contactos"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:textSize="20sp"
        android:text="@string/contactos_de_confianza"
        />

    <LinearLayout
        android:id="@+id/frame_notificaciones"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_notficaciones"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:text="@string/ajustar_notificaciones"
            />
        <TextView
            android:id="@+id/tv_hora"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical|end"
            android:textSize="20sp"
            tools:text="18:00"
            />

    </LinearLayout>


    <TextView
        android:id="@+id/tv_exportar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:textSize="20sp"
        android:text="@string/exportar_datos"
        />


</LinearLayout>