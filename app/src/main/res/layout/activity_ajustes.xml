<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="12dp"
    android:paddingRight="12dp"
    tools:context=".AjustesActivity"
    android:paddingBottom="12dp"
    android:paddingTop="12dp"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/tv_contactos"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:textSize="20sp"
        android:text="Contactos de confianza"
        />

    <TextView
        android:id="@+id/tv_notificaciones"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:textSize="20sp"
        android:text="Ajustar notificaciones"
        />

    <TextView
        android:id="@+id/tv_exportar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:textSize="20sp"
        android:text="Exportar datos"
        />


</LinearLayout>